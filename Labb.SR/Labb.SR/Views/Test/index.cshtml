@{
    Layout = null;
}
<html>
<head>
    <link href="../../Content/qunit.css" rel="stylesheet" type="text/css" />
    <script src="http://code.jquery.com/jquery-latest.js" type="text/javascript"></script>
    <script src="../../Scripts/qunit.js" type="text/javascript"></script>
    <script src="../../Scripts/sr.js" type="text/javascript"></script>
    <script src="../../Scripts/2011.3.1129/kendo.all.min.js" type="text/javascript"></script>
    <script type="text/javascript">
        // --- Unittest Code goes here ---

        $(document).ready(function () {
            module("Basic Unit Test");
            test("Sample test", function () {
                expect(1);
                equals(divide(4, 2), 2, 'Expected 2 as the result, result was: ' + divide(4, 2));
            });
        });

        var datasource = new kendo.data.DataSource({
            transport: {
                read: {
                    url: "http://sverigesradio.se/xmltv/sr_2012-01-12.xml",
                    dataType: "xml"

                }
            }
        });

        $(document).ready(function () {
            module("SR Test");
            test("Hämta dagens tablå", function () {
                expect(1);
                var tablå = datasource.read();
                notEqual(tablå, null, 'Borde vara någonting...');

            });
        });



    </script>
</head>
<body>
    <h1 id="qunit-header">
        QUnit example</h1>
    <h2 id="qunit-banner">
    </h2>
    <h2 id="qunit-userAgent">
    </h2>
    <ol id="qunit-tests">
    </ol>
</body>
</html>
